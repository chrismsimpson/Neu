class Foo {
    public x: Int64
}

func main() {
    let foo1 = Foo(x: 1)
    let foo2 = Foo(x: 2)

    var weakFoo: weak Foo? = none
    weakFoo = foo1
    printLine("{}", weakFoo!.x)

    weakFoo = foo2
    printLine("{}", weakFoo!.x)

    weakFoo.clear()
    printLine("{}", weakFoo.hasValue())
}
