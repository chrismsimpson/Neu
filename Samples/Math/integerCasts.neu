func main() {
    let a = 500 as saturated UInt8
    let b = 257 as truncated UInt8
    let c = 127 as! Int8
    let d = -127 as! Int8
    let e = -200 as saturated Int8
    let f = -127 as truncated Int8
    let g = 128 as truncated Int8

    // as Should not eat the token after the type
    let h = (1 as! Int8)

    printLine("{}", a)
    printLine("{}", b)
    printLine("{}", c)
    printLine("{}", d)
    printLine("{}", e)
    printLine("{}", f)
    printLine("{}", g)
    printLine("{}", h)
}
