struct Rectangle {
    width: Int64
    height: Int64

    func grow(var this) {
        this.width *= 2;
        this.height *= 2;
    }

    func area(this) => this.width * this.height
}

func main() {
    var rect = Rectangle(width: 4, height: 8)

    rect.grow()

    printLine("{}", rect.area())
}